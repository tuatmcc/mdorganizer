{
  "name": "mdorganizer",
  "version": "2.0.0",
  "description": "A cli tool to convert Markdown into TypeScript Module",
  "author": "tuatmcc",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/tuatmcc/mdorganizer",
    "type": "git"
  },
  "exports": "./dist/index.bundle.js",
  "types": "./lib/type.d.ts",
  "type": "module",
  "scripts": {
    "build": "node ./scripts/build.js",
    "start": "node ./bin/cli.js",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint ./src/*  --fix && prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "bin": {
    "mdorganizer": "./bin/cli.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "glob": "^10.2.6",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "esbuild": "^0.17.19",
    "esbuild-node-externals": "^1.7.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vitest": "^0.34.6"
  }
}
