{
  "name": "@tuatmcc/mdorganizer",
  "version": "1.1.0",
  "description": "minimalistic markdown organizer and converter",
  "author": "tuatmcc",
  "license": "MIT",
  "exports": "./src/type.d.ts",
  "types": "./src/type.d.ts",
  "type": "module",
  "scripts": {
    "setup": "npm i && npx husky-init && husky install",
    "build": "node ./scripts/build.js",
    "start": "node ./bin/cli.js",
    "lint": "eslint .",
    "lint:fix": "eslint ./src/*  --fix && prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "bin": {
    "mdorganizer": "./bin/cli.js"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "dependencies": {
    "glob": "^10.2.6",
    "gray-matter": "^4.0.3",
    "rehype-format": "^4.0.1",
    "rehype-katex": "^6.0.3",
    "rehype-pretty-code": "^0.9.5",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "esbuild": "^0.17.19",
    "esbuild-node-externals": "^1.7.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
