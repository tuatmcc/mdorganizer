{
  "name": "mdorganizer",
  "version": "2.3.3",
  "description": "A cli tool to convert Markdown into TypeScript Module",
  "author": "tuatmcc",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/tuatmcc/mdorganizer",
    "type": "git"
  },
  "exports": "./dist/index.bundle.mjs",
  "types": "./lib/types.d.ts",
  "type": "module",
  "scripts": {
    "build": "node ./scripts/build.js",
    "start": "node ./bin/cli.js",
    "main": "tsx ./src/main.ts",
    "lint": "biome lint . && biome format .",
    "lint:fix": "biome check --apply .",
    "test": "vitest"
  },
  "bin": {
    "mdorganizer": "./bin/cli.js"
  },
  "packageManager": "npm@10.1.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "esbuild": "^0.17.19",
    "glob": "^10.2.6",
    "gray-matter": "^4.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/parser": "^6.7.4",
    "esbuild-node-externals": "^1.7.0",
    "tsx": "^4.1.2",
    "vitest": "^0.34.6"
  }
}
